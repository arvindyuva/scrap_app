
	<% @scrap_contents.each do |scrap|%>
		<div class="main-box">
			<h3>Source Url</h3>
			<%= scrap.main_url%>
			<ul class="content-box">
				<h4>Link Contect</h4>
				<% scrap.link_contents.each do |link| %>
					<li><%=link.link%></li>
				<%end%>

				<h4>H1 Contect</h4>
				<% scrap.h1_contents.each do |h1_content| %>
					<li><%=h1_content.h1_data%></li>
				<%end%>

				<h4>H2 Contect</h4>
				<% scrap.h2_contents.each do |h2_content| %>
					<li><%=h2_content.h2_data%></li>
				<%end%>

				<h4>H3 Contect</h4>
				<% scrap.h3_contents.each do |h3_content| %>
					<li><%=h3_content.h3_data%></li>
				<%end%>
			</ul>
		</div>
	<%end%>
</div>

<style type="text/css">

	.main-box{
		width: 49%;
    float: left;

	}
	.content-box{
		border: 1px solid #ccc;
		margin-right: 10px;
    height: 300px;

	}
	
</style>